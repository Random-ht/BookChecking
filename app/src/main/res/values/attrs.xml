<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义的actionBar-->
    <declare-styleable name="my_action_bar">
        <attr name="left_image" format="reference" />
        <attr name="left_image_vis" format="integer" />
        <attr name="right_image" format="reference" />
        <attr name="right_image_vis" format="integer" />
        <attr name="left_title" format="string" />
        <attr name="left_title_vis" format="integer" />
        <attr name="center_title" format="string" />
        <attr name="center_title_vis" format="integer" />
        <attr name="right_title" format="string" />
        <attr name="right_title_vis" format="integer" />
    </declare-styleable>

    <declare-styleable name="book_info_item">
        <attr name="left_content" format="string" />
        <attr name="right_content" format="string" />
    </declare-styleable>
</resources>